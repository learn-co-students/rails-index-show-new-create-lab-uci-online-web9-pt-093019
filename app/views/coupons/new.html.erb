<h3>New Coupon Form</h3>
<%= form_tag coupons_path do %>

    <p>Store: <%= text_field_tag :'coupon[store]' %></p>
    <p>Coupon Code: <%= text_field_tag :'coupon[coupon_code]' %></p>

    <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
    <%= submit_tag "Submit Coupon" %>

<% end %>